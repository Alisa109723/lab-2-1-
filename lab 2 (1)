#include <stdio.h>

int main() {
    int num1, num2;
    int sum_cubed;
    float division_result;
    int product;

printf("Введіть перше ціле число: ");
scanf("%d", &num1);

printf("Введіть друге ціле число: ");
scanf("%d", &num2);

sum_cubed = (num1 + num2) * (num1 + num2) * (num1 + num2);

if (num1 != 0)  {
    division_result = (float)num2/ num1;
 } else {
     printf("Ділення на нуль не можливе!\n");
     return 1; 
 }

product = num1 * num2;

printf("Сума чисел у кубі: %d\n", sum_cubed);
printf("Результат ділення другого числа на перше: %.2f\n", division_result);
printf("Добуток чисел: %d\n", product);
return 0;
}
