#include <stdio.h>

int main() {
    int num1, num2;

    // Запитуємо у користувача два цілі числа
    printf("Введіть перше ціле число: ");
    scanf("%d", &num1);

    printf("Введіть друге ціле число: ");
    scanf("%d", &num2);

    // Виводимо результати
    printf("Сума кубів чисел %d і %d: %d\n", num1, num2, (num1 * num1 * num1) + (num2 * num2 * num2));
    printf("Результат ділення 2-го числа на 1-е: %.2f\n", (num1 != 0) ? (float)num2 / num1 : 0);
    printf("Добуток чисел %d і %d: %d\n", num1, num2, num1 * num2);

    return 0;
}
